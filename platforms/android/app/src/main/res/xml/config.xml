<?xml version='1.0' encoding='utf-8'?>
<widget id="com.technoTitans.destinationTitan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>DestinationTitan</name>
    <description>
        A game created by FRC team 1683, the Techno Titans, to spread the word of FIRST to the world.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Techno Titans
    </author>
    <icon platform="android" src="res/icon/android/ic_launcher.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
