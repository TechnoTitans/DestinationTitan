<?xml version='1.0' encoding='utf-8'?>
<widget id="com.technoTitans.destinationTitan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DestinationTitan</name>
    <description>
        A game created by FRC team 1683, the Techno Titans, to spread the word of FIRST to the world.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Techno Titans
    </author>
    <icon platform="android" src="res/icon/android/ic_launcher.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.1.4" />
</widget>
